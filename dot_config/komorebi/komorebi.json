{
  "$schema": "https://raw.githubusercontent.com/LGUG2Z/komorebi/v0.1.37/schema.json",
  "app_specific_configuration_path": "%USERPROFILE%/.config/komorebi/applications.json",
  "window_hiding_behaviour": "Cloak",
  "cross_monitor_move_behaviour": "Insert",
  "default_workspace_padding": 10,
  "default_container_padding": 10,
  "animation": {
    "enabled": true,
    "type": "EaseInOutSine",
    "duration": 400,
  },
  "border": true,
  "border_width": 5,
  "border_offset": -1,
  "stackbar": {
    "mode": "Never",
  },
  "transparency": true,
  "transparency_alpha": 235,
  "theme": {
    "palette": "Custom",
    "colours": {
      "base_00": "#1c1110",
      "base_01": "#e2935b",
      "base_02": "#c8594f",
      "base_03": "#8f4d47",
      "base_04": "#b13756",
      "base_05": "#b9827f",
      "base_06": "#845b49",
      "base_07": "#392621",
      "base_08": "#845b49",
      "base_09": "#47211f",
      "base_0a": "#47211f",
      "base_0b": "#47211f",
      "base_0c": "#47211f",
      "base_0d": "#47211f",
      "base_0e": "#47211f",
      "base_0f": "#47211f",
    },
    "unfocused_border": "Base02",
    "bar_accent": "Base01"
  },
  "monitors": [
    {
      "workspaces": [
        {
          "name": "I",
          "layout": "BSP"
        },
        {
          "name": "II",
          "layout": "BSP"
        },
        {
          "name": "III",
          "layout": "BSP"
        },
        {
          "name": "IV",
          "layout": "BSP"
        },
        {
          "name": "V",
          "layout": "BSP"
        },
        {
          "name": "VI",
          "layout": "Grid"
        },
      ]
    }
  ]
}
